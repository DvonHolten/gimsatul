CC=@CC@
CFLAGS=@CFLAGS@
all: gimsatul
clean:
	rm -f makfile config.h gimsatul *~ *.backup
indent:
	indent gimsatul.c
test: all
	cnf/test.sh
docker: clean
	docker build -t gimsatul .
gimsatul: gimsatul.c config.h
	$(CC) $(CFLAGS) -o $@ $< -lm -pthread
config.h: makefile VERSION
	./mkconfig.sh > $@
.PHONY: all clean doacker indent test
